CREATE TABLE workouts (
workout_no INT PRIMARY KEY AUTO_INCREMENT, 
name VARCHAR(255), 
sets TINYINT UNSIGNED, 
reps TINYINT UNSIGNED, 
weight_lbs SMALLINT UNSIGNED, 
date_time DATETIME, 
duration_minutes SMALLINT UNSIGNED);

